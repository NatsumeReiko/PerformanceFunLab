<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="lab.fun.performance" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".javaheap.HeapObjectCountTest"
            android:label="Java Heap/HeapObjectCountTest">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="lab.fun.performance.SAMPLE_CODE" />

            </intent-filter>

        </activity>

        <activity android:name=".javaheap.TargetTextViewHeapObjectCountTest"
            android:label="Java Heap/TargetTextViewHeapObjectCountTest">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="lab.fun.performance.SAMPLE_CODE" />

            </intent-filter>

        </activity>
    </application>

</manifest>
